<tr data-contribution-id="<%= contribution.id %>">
  <td><%= t("activerecord.models.#{contribution.approvable_type.downcase}").camelize %></td>
  <td><%= t_enum contribution, :event %></td>
  <td><%= contribution.creator_pseudo %></td>
  <td><%= l contribution.created_at, :format => :short %></td>
  <td>
  	<% case contribution.approvable_type
  		 when Artist.name %>
				<%= render :partial => "artist_line_block", :locals => { artist: contribution.object } %>
		<% when Album.name %>
				<%= render :partial => "product_line_block", :locals => { product: contribution.object } %>
			<% end %>
  </td>
</tr>

